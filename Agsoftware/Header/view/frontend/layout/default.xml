<page layout="3columns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <link src="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&amp;display=swap" src_type="url" /> 
        <css src="Agsoftware_Header::css/bootstrap-icons.css"/>
        <css src="Agsoftware_Header::css/hs-mega-menu.min.css"/>
        <css src="Agsoftware_Header::css/theme.min.css"/>

    </head>
    <body>
        <referenceContainer name="header-wrapper" >
            <block class="Magento\Theme\Block\Html\Header\Logo" name="links-custom"
                   template="Agsoftware_Header::links-custom.phtml" after="logo"/>
        </referenceContainer>
        <referenceBlock name="minicart" remove="true"/>
        <referenceBlock name="top.search" remove="true"/>
        <referenceBlock name="navigation.sections">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Agsoftware_Header::sections.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="logo" remove="true"/> 
        <referenceBlock name="header.panel.wrapper" remove="true" />
        <referenceContainer name="copyright" remove="true"/>

        <referenceContainer name="header-wrapper" remove="false"/>     
    </body>
</page>
